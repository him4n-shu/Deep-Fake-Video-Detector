{
  "name": "veritas-ai",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://deepfake-backend-production-de93.up.railway.app/$1"
    }
  ],
  "env": {
    "VITE_API_BASE_URL": "https://deepfake-backend-production-de93.up.railway.app"
  }
}
